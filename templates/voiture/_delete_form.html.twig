<form method="post" action="{{ path('voiture_delete', {'id': voiture.id}) }}" onsubmit="return
confirm('Voulez-vous supprimer cet élément ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ voiture.id) }}">
    <button id="js-delete" class="btn btn-danger">Supprimer</button>
</form>
